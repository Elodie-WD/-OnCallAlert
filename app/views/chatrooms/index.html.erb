<div class="container">
  <h1>All Chatrooms</h1>

  <div class="list-group">
    <% @chatrooms.each do |chatroom| %>
      <div class="list-group-item">
        <h2><%= link_to chatroom.name, chatroom_path(chatroom) %></h2>
      </div>
    <% end %>
  </div>
</div>

