<div class="container-map">
  <div class="text-en-tete-map">
    <div class="row">
      <div class="col">
        <%= link_to "Retour", rooting_path(), class: "text-historique" %>
      </div>
      <div class="col">
        <h2 class="title-historique">Itinéraire</h2>
      </div>
    </div>
  </div>
    <div
      data-controller="map"
      data-map-markers-value="<%= @markers.to_json %>"
      data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"
      data-map-latitude-value="<%= @contact.latitude%>"
      data-map-longitude-value="<%= @contact.longitude%>"
      data-map-hospital-latitude-value="<%= @contact.department.hospital.latitude%>"
      data-map-hospital-longitude-value="<%= @contact.department.hospital.longitude%>">
      <div data-map-target="box" style="width: 100%; height: 550px;"></div>
    <h3>L'astreinte est en route ! </h3>
    <h4>Temps estimé : <span class="duration" data-map-target="duration"> </span> mins </h4>
    </div>
</div>
