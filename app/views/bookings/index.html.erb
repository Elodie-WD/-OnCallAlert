<div class="container" data-controller="calendar">

  <div class="calendar" id="calendrier">
  <%= calendar(number_of_days: 5) do |date| %>
      <div class="day" data-action="click->calendar#getUsers">
        <%= date %>
      </div>
  <% end %>
  </div>

  <div class="container-calendar">
    <div class="box1">
      <img src="<%= image_path("avatar1.png") %>" style="width: 40px;" class="avatar bg-info rounded-circle" />
      <p data-calendar-target="degardeJobTitle" id="nom">Pr</p>
      <p data-calendar-target="degardeLastName" id="nom">Marie Moreau</p>
      <p id="metier">Obstétricien</p>
      <p id="status"> De garde </p>
    </div>
    <div class="box2">
      <img src="<%= image_path("avatar1.png") %>" style="width: 40px;" class="avatar bg-info rounded-circle" />
      <p data-calendar-target="astreinteJobTitle" id="nom">Dr</p>
      <p data-calendar-target="astreinteLastName" id="nom">Marie Moreau</p>
      <p id="metier">Obstétricien</p>
      <p id="status"> D'astreinte </p>
    </div>
  </div>
</div>
