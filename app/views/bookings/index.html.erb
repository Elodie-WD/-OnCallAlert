<div class="container" data-controller="calendar">

<div class="background-blue-calendrier">
</div>
  <div class="calendar" id="calendrier">
  <%= month_calendar do |date| %>
      <div class="day" data-action="click->calendar#getUsers" data-date="<%= date %>">
        <%= date.day %>
      </div>
  <% end %>
  </div>




  <div class="container-calendar">
    <div class="box1">

      <div class="namecenter">
        <p data-calendar-target="degardeFirstName" id="nom">Dr Alice</p>
        <p data-calendar-target="degardeLastName" id="nom">ROUSSEL</p>
      </div>
      <p id="metier">Service d'Obstétrique</p>
      <p id="status"> De garde </p>
    </div>
    <div class="box2">
      <div class="namecenter">
        <p data-calendar-target="astreinteFirstName" id="nom">Dr Benoît</p>
        <p data-calendar-target="astreinteLastName" id="nom">DALLET</p>
      </div>
      <p id="metier">Service d'Obstétrique</p>
      <p id="status"> D'astreinte </p>
    </div>
  </div>
</div>
